  "bugs": {
    "url": "https://github.com/abdulmujeeb/hellonode/issues"
  },
  "homepage": "https://github.com/abdulmujeeb/hellonode#readme",
  "dependencies": {
    "bootstrap": "4.3.1",
    "chai": "4.2.0",
    "chalk": "2.4.2",
    "cors": "2.8.5",
    "debug": "4.1.1",
    "ejs": "2.6.2",
    "express": "4.17.1",
    "jimp": "0.2.28",
    "jquery": "3.4.1",
    "mocha": "6.2.0",
    "morgan": "1.9.1",
    "nodemon": "1.19.1",
